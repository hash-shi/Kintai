<?xml version="1.0" encoding="UTF-8"?>
<validate>
	<proc func="download">
	
		<variable name="srhTxtTaishoNengetsuF" dispName="対象年月度(自)">
			<check class="IsRequired" message="!#itemName#!が入力されていません。" itemName="{dispName}"></check>
			<check class="IsDate" message="!#itemName#!は日付フォーマットで入力してください。" itemName="{dispName}">
				<param name="format">yyyy/MM</param>
			</check>
		</variable>
		
		<variable name="srhTxtTaishoNengetsuT" dispName="対象年月度(至)">
			<check class="IsRequired" message="!#itemName#!が入力されていません。" itemName="{dispName}"></check>
			<check class="IsDate" message="!#itemName#!は日付フォーマットで入力してください。" itemName="{dispName}">
				<param name="format">yyyy/MM</param>
			</check>
		</variable>
		
		<variable name="dummy" dispName="">
			<check class="SizeComparison" message="対象年月度(自)(至)の大小が不正です。" itemName="{dispName}">
				<param name="from">srhTxtTaishoNengetsuF</param>
				<param name="to">srhTxtTaishoNengetsuT</param>
				<param name="include">1</param>
			</check>
		</variable>
		
	</proc>
</validate>