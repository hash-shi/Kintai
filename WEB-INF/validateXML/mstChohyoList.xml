<?xml version="1.0" encoding="UTF-8"?>
<validate>
	<proc func="eigyosho">
		<variable name="srhTxtEigyoshoCodeF" dispName="営業所コードFROM">
		    <check class="IsHalf" message="!#itemName#!には全角文字は入力できません。" itemName="{dispName}"></check>
		    <check class="IsNumber" message="!#itemName#!には数字を入力してください。" itemName="{dispName}"></check>
		    <check class="MaxLength" message="!#itemName#!が3バイトを超えています。(3バイト = 全角3/2文字, 半角3文字)" itemName="{dispName}">
		    	<param name="type">full</param>
				<param name="length">3</param>
			</check>
		</variable>
		<variable name="srhTxtEigyoshoCodeT" dispName="営業所コードTO">
		    <check class="IsHalf" message="!#itemName#!には全角文字は入力できません。" itemName="{dispName}"></check>
		    <check class="IsNumber" message="!#itemName#!には数字を入力してください。" itemName="{dispName}"></check>
		    <check class="MaxLength" message="!#itemName#!が3バイトを超えています。(3バイト = 全角3/2文字, 半角3文字)" itemName="{dispName}">
		    	<param name="type">full</param>
				<param name="length">3</param>
			</check>
		</variable>
		<variable name="dummy" dispName="">
             <check class="SizeComparison" message="営業所コードFROM-TOの大小が不正です。" itemName="{dispName}">
                 <param name="from">srhTxtEigyoshoCodeF</param>
                 <param name="to">srhTxtEigyoshoCodeT</param>
                 <param name="include">1</param>
             </check>
        </variable>
        <variable name="srhTxtSaishuKoshinDateF" dispName="最終更新日FROM">
			<check class="IsDate" message="!#itemName#!の指定が不正です。(YYYY/MM/DD)" itemName="{dispName}">
                <param name="format">yyyy/MM/dd</param>
            </check>
		</variable>
		<variable name="srhTxtSaishuKoshinDateT" dispName="最終更新日TO">
			<check class="IsDate" message="!#itemName#!の指定が不正です。(YYYY/MM/DD)" itemName="{dispName}">
                <param name="format">yyyy/MM/dd</param>
            </check>
		</variable>
		<variable name="dummy" dispName="">
            <check class="SizeComparison" message="最終更新日FROM-TOの大小が不正です。" itemName="{dispName}">
                 <param name="from">srhTxtSaishuKoshinDateF</param>
                 <param name="to">srhTxtSaishuKoshinDateT</param>
                 <param name="include">1</param>
            </check>
        </variable>
     </proc>
     
