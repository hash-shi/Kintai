<?xml version="1.0" encoding="UTF-8"?>
<validate>
	<proc func="search">
		<variable name="srhTxtEigyoshoCode" dispName="営業所コード(検索)">
			<check class="IsRequired" message="!#itemName#!が入力されていません。" itemName="{dispName}"></check>
			<check class="IsHalf" message="!#itemName#!には全角文字は入力できません。" itemName="{dispName}"></check>
			<check class="IsNumber" message="!#itemName#!には数字を入力してください。" itemName="{dispName}"></check>
		</variable>
	</proc>
	
	<proc func="delete">
		<variable name="txtEigyoshoCode" dispName="営業所コード">
			<check class="IsEquals" message="コピー機能使用時は削除できません。" itemName="{dispName}">
				<param name="targetvalue">hdnEigyoshoCode</param>
			</check>
		</variable>
	</proc>
	
	<proc func="insert,update">
		<variable name="txtEigyoshoCode" dispName="営業所コード">
			<check class="IsRequired" message="!#itemName#!が入力されていません。" itemName="{dispName}"></check>
			<check class="IsHalf" message="!#itemName#!には全角文字は入力できません。" itemName="{dispName}"></check>
			<check class="IsNumber" message="!#itemName#!には数字を入力してください。" itemName="{dispName}"></check>
		</variable>
		<variable name="txtEigyoshoName" dispName="営業所名">
			<check class="IsRequired" message="!#itemName#!が入力されていません。" itemName="{dispName}"></check>			
			<check class="MaxLength" message="!#itemName#!が20バイトを超えています。(20バイト = 全角20/2文字, 半角20文字)" itemName="{dispName}">
				<param name="type">full</param>
				<param name="length">20</param>
			</check>
		</variable>
	</proc>
	
	<proc func="delete_,insert_,update_">
		<variable name="dummy" dispName="">
			<check class="ExclusiveValidate" message="このデータはすでに、別のユーザに更新されています。もう一度データを確認してください。" itemName="{dispName}">
				<param name="tableName">MST_EIGYOSHO</param>
				<param name="keyName">EigyoshoCode</param>
				<param name="keyValue">txtEigyoshoCode</param>
				<param name="saishuKoshinDate">hdnSaishuKoshinDate</param>
				<param name="saishuKoshinJikan">hdnSaishuKoshinJikan</param>
			</check>
		</variable>
	</proc>
	
	<proc func="copy_">
		<variable name="dummy" dispName="">
			<check class="ExclusiveValidate" message="同一キーのレコードが既に存在します。" itemName="{dispName}">
				<param name="tableName">MST_EIGYOSHO</param>
				<param name="keyName">EigyoshoCode</param>
				<param name="keyValue">txtEigyoshoCode</param>
				<param name="saishuKoshinDate"></param>
				<param name="saishuKoshinJikan"></param>
			</check>			
		</variable>
	</proc>
</validate>