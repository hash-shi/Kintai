<?xml version="1.0" encoding="UTF-8"?>

<validate>
	<proc func="search">
		<variable name="srhTxtShainNO" dispName="社員NO(検索)">
			<check class="IsRequired" message="!#itemName#!が入力されていません。" itemName="{dispName}"></check>
			<check class="IsHalf" message="!#itemName#!には全角文字は入力できません。" itemName="{dispName}"></check>			
			<check class="IsNumber" message="!#itemName#!には数字を入力してください。" itemName="{dispName}"></check>
			<check class="MaxLength" message="!#itemName#!が4バイトを超えています。(4バイト = 全角4/2文字, 半角4文字)" itemName="{dispName}">
				<param name="type">full</param>
				<param name="length">4</param>
			</check>
		</variable>
	</proc>
	
	<proc func="insert,update">
	<!-- 社員NO -->
		<variable name="txtShainNO" dispName="社員NO">
			<check class="IsRequired" message="!#itemName#!が入力されていません。" itemName="{dispName}"></check>
			<check class="IsHalf" message="!#itemName#!には全角文字は入力できません。" itemName="{dispName}"></check>			
			<check class="IsNumber" message="!#itemName#!には数字を入力してください。" itemName="{dispName}"></check>
			<check class="MaxLength" message="!#itemName#!が4バイトを超えています。(4バイト = 全角4/2文字, 半角4文字)" itemName="{dispName}">
				<param name="type">full</param>
				<param name="length">4</param>
			</check>		
		</variable>
		
	<!-- 社員名 -->	
		<variable name="txtShainName" dispName="社員名">
			<check class="IsRequired" message="!#itemName#!が入力されていません。" itemName="{dispName}"></check>			
			<check class="MaxLength" message="!#itemName#!が20バイトを超えています。(20バイト = 全角20/2文字, 半角20文字)" itemName="{dispName}">
				<param name="type">full</param>
				<param name="length">20</param>
			</check>
		</variable>
		
	<!-- パスワード -->	
		<variable name="txtPassword" dispName="パスワード">
			<check class="IsRequired" message="!#itemName#!が入力されていません。" itemName="{dispName}"></check>
			<check class="MinLength" message="!#itemName#!は1文字以上、50文字以下で入力してください。" itemName="{dispName}">
				<param name="type">half</param>
				<param name="length">1</param>
			</check>
			<check class="MaxLength" message="!#itemName#!は1文字以上、50文字以下で入力してください。" itemName="{dispName}">
				<param name="type">half</param>
				<param name="length">50</param>
			</check>
			
    		<check class="IsRegex" message="!#itemName#!は英字、数字、記号(_-.+$#!/@)で入力してください。" itemName="{dispName}">
		        <param name="format">^[A-Za-z0-9_\-\.\+\$#!/@]{1,50}$</param>
		    </check>
		    <check class="IsRegex" message="!#itemName#!は英字、数字、記号(_-.+$#!/@)を2種類以上含めて入力してください。" itemName="{dispName}">
		        <param name="format">^((?=.*[A-Za-z])(?=.*[0-9])|(?=.*[A-Za-z])(?=.*[\-\.\+\$#!/@])|(?=.*[0-9])(?=.*[\-\.\+\$#!/@]))([A-Za-z0-9_\-\.\+\$#!/@]){1,50}$</param>
		    </check>		    
		    <check class="IsNotEquals" message="!#itemName#!が正しくありません。">
		        <param name="targetvalue">txtShainNO</param>
		    </check>	
		</variable>		
		
	<!-- ユーザ区分 -->
