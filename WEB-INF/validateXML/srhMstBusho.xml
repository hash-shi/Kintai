		<variable name="srhTxtEigyoshoCode" dispName="営業所コード">
			<check class="IsRegex" message="!#itemName#!には数字を入力してください" itemName="{dispName}">
				<param name="format">^$|^[0-9]{1,50}$</param>
			</check>
		</variable>
		
<		<variable name="srhTxtBushoCode" dispName="部署コード">
			<check class="IsRegex" message="!#itemName#!には数字を入力してください" itemName="{dispName}">
				<param name="format">^$|^[0-9]{1,50}$</param>
			</check>
		</variable>
	</proc>
</validate>